<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="java.devcolibri.itvdn.com.hellotanks">

    <application
        android:allowBackup="true"
        android:icon="@drawable/golden_tank_up"
        android:label="@string/app_name"
        android:roundIcon="@drawable/golden_tank_up"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.GameActivity" />
        <activity android:name=".activity.CreateMapActivity" />
        <activity android:name=".activity.ChoseMapActivity" />
    </application>
</manifest>
